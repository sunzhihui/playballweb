/* task_content */
.task_content{
	width: 100%;
	padding: 0 30rpx;
	background-color: #ffffff;
	margin-top: 30rpx;
	padding-bottom: 10rpx;
}
.content_top{
	width: 100%;
	height: 82rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.red_line{
	width: 8rpx;
	height: 30rpx;
	border-radius: 4rpx;
	background-color: #ff0000;
}
.content_title{
	width: 341rpx;
	height: 82rpx;
	line-height: 82rpx;
	color: #333333;
	padding-left: 15rpx;
	font-size: 30rpx;
}
.task_plan{
	width: 341rpx;
	height: 82rpx;
	line-height: 82rpx;
	font-size: 26rpx;
	text-align: right;
}
.task_award{
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.task_award_list{
	width: 230rpx;
}
.award_goal{
	width: 116rpx;
	height: 144rpx;
	background-image: url(data:image/png;base64,);
	background-size: cover;
	background-position: 50% 50%;
	margin: 0 auto;
	padding-top: 70rpx;
}
.award_goal text{
	display: block;
	width: 100%;
	text-align: center;
	height: 38rpx;
	line-height: 38rpx;
	font-size: 24rpx;
	color: #E8E8E6;
}
.award_goal ._span{
	display: block;
	width: 100%;
	text-align: center;
	height: 30rpx;
	line-height: 30rpx;
	font-size: 22rpx;
	color: #E8E8E6;
}
.plan_line{
	width: 100%;
	height: 38rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	margin-top: 20rpx;
	position: relative;
}
.plan_line_lf{
	width: 115rpx;
	height: 10rpx;
	/* background-color: #FFE1DE; */
	background: -webkit-gradient(linear, left top, right top, from(#FF3826), to(#FFB2AA));
	background: -o-linear-gradient(left, #FF3826, #FFB2AA);
	background: linear-gradient(left, #FF3826, #FFB2AA);
}
.plan_line_circle{
	width: 38rpx;
	height: 38rpx;
	border-radius: 50%;
	border: 4rpx #FD3C2B solid;
	background-color: #FFD5D1;
	position: absolute;
	top: 0;
	left: 50%;
	margin-left: -19rpx;
}
.plan_line_rt{
	width: 115rpx;
	height: 10rpx;
	/* background-color: #ffe1de; */
	background: -webkit-gradient(linear, right top, left top, from(#FF3826), to(#FFB2AA));
	background: -o-linear-gradient(right, #FF3826, #FFB2AA);
	background: linear-gradient(right, #FF3826, #FFB2AA);
}
.task_award_list:first-of-type .plan_line .plan_line_lf{
	border-radius: 5rpx 0 0 5rpx;
}
.task_award_list:last-of-type .plan_line .plan_line_rt{
	border-radius: 0 5rpx 5rpx 0;
}
.active .plan_line .plan_line_rt{
	background: -webkit-gradient(linear, left top, right top, from(#ffe1de), to(#ffe1de));
	background: -o-linear-gradient(left, #ffe1de, #ffe1de);
	background: linear-gradient(left, #ffe1de, #ffe1de);
}
.active~.task_award_list .plan_line .plan_line_circle{
	border-color: #FEB5AE;
}
.active~.task_award_list .plan_line .plan_line_lf{
	background: -webkit-gradient(linear, left top, right top, from(#ffe1de), to(#ffe1de));
	background: -o-linear-gradient(left, #ffe1de, #ffe1de);
	background: linear-gradient(left, #ffe1de, #ffe1de);
}
.active~.task_award_list .plan_line .plan_line_rt{
	background: -webkit-gradient(linear, left top, right top, from(#ffe1de), to(#ffe1de));
	background: -o-linear-gradient(left, #ffe1de, #ffe1de);
	background: linear-gradient(left, #ffe1de, #ffe1de);
}
